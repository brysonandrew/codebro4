{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "array-type": false,
    "prefer-const": false,
    "variable-name": [false],
    "no-string-literal": false,
    "new-parens": false,
    "jsx-no-lambda": false,
    "no-empty-interface": false,
    "no-namespace": [false],
    "interface-name": [false],
    "class-name": false,
    "max-line-length": [false],
    "object-literal-shorthand": false,
    "member-access": [false],
    "no-trailing-whitespace": [false],
    "arrow-parens": false,
    "jsx-wrap-multiline": {
      "when": "ignore"
    },
    "jsx-no-multiline-js": {
      "when": "ignore"
    },
    "jsx-curly-spacing": {
      "when": "ignore"
    },
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "ignore",
          "arrays": "ignore",
          "functions": "ignore",
          "typeLiterals": "ignore"
        }
      }
    ],
    "indent": false,
    "semicolon": false,
    "quotemark": [
      true,
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ]
  }
}
